  "scripts": {
    "start": "craco start",
    "build": "npm run optimize:assets && craco build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run optimize:assets && craco build",
    "build:ssr": "npm run build && npm run build:ssr-server",
    "build:ssr-server": "babel src/server --out-dir build/server --copy-files",
    "start:ssr": "node build/server/ssr-server.js",
    "start:ssr-dev": "nodemon src/server/ssr-server.js --exec babel-node",
    "optimize:assets": "npm run optimize:fonts && node ../scripts/optimize-assets.js",
    "optimize:images": "node ../scripts/optimize-assets.js --images-only",
    "optimize:fonts": "node ../scripts/optimize-fonts.js",
    "analyze": "webpack-bundle-analyzer build/static/js/*.js",
    "analyze:webpack": "webpack --config webpack.analyze.js",
    "build:optimized": "webpack --config webpack.optimized.js",
    "test": "craco test --watchAll=false",
    "test:watch": "craco test",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:debug": "craco test --watchAll=false --verbose",
    "test:ci": "craco test --coverage --watchAll=false --ci --passWithNoTests",
    "test:visual": "craco test --testRegex=\"(\\.|/)visual\\.(test|spec)\\.(ts|tsx|js|jsx)$\" --watchAll=false",
    "test:visual:update": "craco test --testRegex=\"(\\.|/)visual\\.(test|spec)\\.(ts|tsx|js|jsx)$\" --watchAll=false --updateSnapshot",
    "test:visual:coverage": "craco test --testRegex=\"(\\.|/)visual\\.(test|spec)\\.(ts|tsx|js|jsx)$\" --coverage --watchAll=false",
    "test:a11y": "craco test --testRegex=\"(\\.|/)(a11y|accessibility)\\.(test|spec)\\.(ts|tsx|js|jsx)$\" --watchAll=false",
    "test:a11y:watch": "craco test --testRegex=\"(\\.|/)(a11y|accessibility)\\.(test|spec)\\.(ts|tsx|js|jsx)$\"",
    "test:a11y:coverage": "craco test --testRegex=\"(\\.|/)(a11y|accessibility)\\.(test|spec)\\.(ts|tsx|js|jsx)$\" --coverage --watchAll=false",
    "test:a11y:ci": "tsx src/components/Charts/accessibility/AccessibilityCIIntegration.ts",
    "test:a11y:rover": "tsx src/components/Charts/accessibility/AccessibilityCIIntegration.ts --config rover",
    "test:a11y:report": "tsx src/components/Charts/accessibility/AccessibilityCIIntegration.ts --report-only",
    "lighthouse:ci": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:upload": "lhci upload",
    "test:performance": "node src/components/layout/MissionControl/LODSystem/__tests__/performance/ci-performance-test.js",
    "test:performance:local": "craco test --testNamePattern=\"Performance\" --watchAll=false",
    "test:performance:watch": "craco test --testNamePattern=\"Performance\"",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
